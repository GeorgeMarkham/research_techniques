@article{HOUMANI2012993,
title = "BioSecure signature evaluation campaign (BSEC'2009): Evaluating online signature algorithms depending on the quality of signatures",
journal = "Pattern Recognition",
volume = "45",
number = "3",
pages = "993 - 1003",
year = "2012",
issn = "0031-3203",
doi = "https://doi.org/10.1016/j.patcog.2011.08.008",
url = "http://www.sciencedirect.com/science/article/pii/S0031320311003220",
author = "N. Houmani and A. Mayoue and S. Garcia-Salicetti and B. Dorizzi and M.I. Khalil and M.N. Moustafa and H. Abbas and D. Muramatsu and B. Yanikoglu and A. Kholmatov and M. Martinez-Diaz and J. Fierrez and J. Ortega-Garcia and J. Roure Alcobé and J. Fabregas and M. Faundez-Zanuy and J.M. Pascual-Gaspar and V. Cardeñoso-Payo and C. Vivaracho-Pascual",
keywords = "On-line signature verification, Evaluations, Signature Categorization, Quality, Entropy",
abstract = "In this paper, we present the main results of the BioSecure Signature Evaluation Campaign (BSEC'2009). The objective of BSEC'2009 was to evaluate different online signature algorithms on two tasks: the first one aims at studying the influence of acquisition conditions (digitizing tablet or PDA) on systems' performance; the second one aims at studying the impact of information content in signatures on systems' performance. In BSEC'2009, the two BioSecure Data Sets DS2 and DS3 are used for tests, both containing data of the same 382 people, acquired respectively on a digitizing tablet and on a PDA. The results of the 12 systems involved in this evaluation campaign are reported and analyzed in detail in this paper. Experimental results reveal a 2.2\% EER for skilled forgeries and a 0.51\% EER for random forgeries on DS2; and a 4.97\% EER for skilled forgeries and a 0.55\% EER for random forgeries on DS3."
}
